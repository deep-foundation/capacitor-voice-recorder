{
  "name": "@deep-foundation/capacitor-voice-recorder",
  "type": "module",
  "keywords": [
    "deep-package",
    "deep-package-integration"
  ],
  "version": "1.2.6",
  "repository": {
    "type": "git",
    "url": "https://github.com/deep-foundation/capacitor-voice-recorder"
  },
  "scripts": {
    "library:build:clean": "fse remove dist && npm run library:build:dirty",
    "library:build:dirty": "ts-node --esm ./automation/library/build.ts",
    "npm-pull": "npm-pull",
    "npm-release": "npm-release",
    "library:build:generate-package-class": "generate-package-class",
    "documentation:generate": "ts-node --esm ./automation/documentation/generate.ts",
    "library:documentation:generate": "typedoc ./src/main.ts"
  },
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "/dist",
    "./deep.json"
  ],
  "dependencies": {
    "@deep-foundation/core": "~0.0.2",
    "@deep-foundation/device": "~1.0.1",
    "@deep-foundation/sound": "~0.0.3",
    "@deep-foundation/tsx": "~0.0.3"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^2.5.5",
    "@deep-foundation/deeplinks": "~0.0.271",
    "capacitor-voice-recorder": "5.0.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@atao60/fse-cli": "^0.1.7",
    "@capacitor/cli": "^5.0.0",
    "@deep-foundation/npm-automation": "^18.1.3",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.2.4",
    "fs-extra": "^11.1.1",
    "typescript": "^5.2.2"
  }
}
